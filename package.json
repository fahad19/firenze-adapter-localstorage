{
  "name": "firenze-adapter-localstorage",
  "version": "0.2.0",
  "description": "localStorage adapter for firenze.js ORM",
  "main": "index.js",
  "scripts": {
    "babel": "babel src --out-dir lib",
    "babel:watch": "babel --watch src --out-dir lib",
    "lint": "eslint src",
    "mocha": "mocha $(find lib/test/cases -name '*.js')",
    "test": "run-scripts lint babel mocha",
    "istanbul": "istanbul cover ./node_modules/mocha/bin/_mocha $(find lib/test/cases -name '*.js')",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "coverage": "run-scripts istanbul coveralls",
    "doctoc": "doctoc README.md --github --title '# Contents'",
    "docume": "docume README.md",
    "docs": "npm run docume && npm run doctoc",
    "dist:lib": "webpack --config ./dist/firenze-adapter-localstorage.config.js",
    "dist:test": "webpack --config ./dist/test.config.js",
    "uglify:lib": "uglifyjs dist/firenze-adapter-localstorage.js -o dist/firenze-adapter-localstorage.min.js",
    "dist": "run-scripts dist:lib uglify:lib",
    "karma-start": "karma start karma.conf.js",
    "karma": "run-scripts babel dist:test karma-start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fahad19/firenze-adapter-localstorage.git"
  },
  "keywords": [
    "firenze",
    "firenze-adapter",
    "firenze-plugin",
    "orm",
    "localstorage",
    "database",
    "adapter"
  ],
  "author": "Fahad Ibnay Heylaal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fahad19/firenze-adapter-localstorage/issues"
  },
  "homepage": "https://github.com/fahad19/firenze-adapter-localstorage",
  "dependencies": {
    "async": "^1.2.1",
    "lodash": "~3.7.0"
  },
  "peerDependencies": {
    "firenze": "~0.2.1"
  },
  "devDependencies": {
    "firenze": "~0.2.1",
    "babel": "^5.1.10",
    "babel-eslint": "^3.0.1",
    "coveralls": "^2.11.2",
    "doctoc": "^0.13.0",
    "docume": "^0.1.4",
    "eslint": "^0.20.0",
    "istanbul": "^0.3.15",
    "karma": "^0.12.36",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.2.0",
    "mocha": "^2.2.4",
    "node-libs-browser": "^0.5.2",
    "phantomjs": "^1.9.17",
    "run-scripts": "^0.2.2",
    "should": "^6.0.1",
    "should-promised": "^0.2.1",
    "uglify-js": "^2.4.23",
    "webpack": "^1.9.11"
  }
}
